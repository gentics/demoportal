CREATE CACHED TABLE CONTENTATTRIBUTETYPE(NAME VARCHAR(32) DEFAULT NULL,ATTRIBUTETYPE INTEGER DEFAULT NULL,OPTIMIZED INTEGER DEFAULT NULL,QUICKNAME VARCHAR(255) DEFAULT NULL,MULTIVALUE INTEGER DEFAULT 0 NOT NULL,OBJECTTYPE INTEGER DEFAULT 0 NOT NULL,LINKEDOBJECTTYPE INTEGER DEFAULT 0 NOT NULL,FOREIGNLINKATTRIBUTE VARCHAR(255) DEFAULT '' NOT NULL,FOREIGNLINKATTRIBUTERULE VARCHAR(32000))
CREATE CACHED TABLE CONTENTMAP(CONTENTID VARCHAR(32) DEFAULT '' NOT NULL PRIMARY KEY,OBJ_ID INTEGER DEFAULT 0 NOT NULL,OBJ_TYPE INTEGER DEFAULT 0 NOT NULL,MOTHERID VARCHAR(32) DEFAULT '' NOT NULL,MOTHER_OBJ_ID INTEGER DEFAULT 0 NOT NULL,MOTHER_OBJ_TYPE INTEGER DEFAULT 0 NOT NULL,UPDATETIMESTAMP INTEGER DEFAULT 0 NOT NULL)
CREATE CACHED TABLE CONTENTOBJECT(TYPE INTEGER DEFAULT NULL,NAME VARCHAR(255) DEFAULT NULL)
CREATE CACHED TABLE CONTENTSTATUS(NAME VARCHAR(255) DEFAULT '' NOT NULL,INTVALUE INTEGER DEFAULT NULL,STRINGVALUE LONGVARCHAR)
